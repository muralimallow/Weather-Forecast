<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#D47753" />
            <gradient android:angle="-90" android:startColor="#852600" android:endColor="#852600" />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#852600" />
            <gradient android:angle="-90" android:startColor="#852600" android:endColor="#852600" />
        </shape>
    </item>
</selector>